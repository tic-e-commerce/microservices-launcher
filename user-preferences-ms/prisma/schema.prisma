generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model ProductFavorite {
  id         String   @id @default(cuid())
  product_id Int
  user_id    Int
  created_at DateTime @default(now())
  updated_at DateTime @updatedAt

  @@map("product_favorites")
}
